        {
          "datasource": {
            "type": "postgres",
            "uid": "${DS_TURBO_TIMESCALE}"
          },
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "WITH last_num_cpus AS\n(\n  select\n  \tdistinct on (e.oid)\n  \te.oid,\n  \te.name,\n  \t(e.attrs->>'num_cpus')::integer as \"num_cpus\",\n  \t(e.attrs->>'tags')::text as \"tags\"\n  from entity e\n  where\n  \te.\"type\" = 'VIRTUAL_MACHINE'\n    AND e.oid IN (${VM_OID})\n  order by e.oid, e.last_seen DESC\n),\navg_num_cpu as\n(\n\tselect\n\t\thea.entity_oid as oid,\n\t\tavg(hea.int_value) as \"num_cpus\"\n\tfrom historical_entity_attrs hea\n\tWHERE\n  \t\t$__timeFilter(hea.time)\n  \t    AND hea.type = 'NUM_VCPU'\n        AND hea.entity_oid IN (${VM_OID})\n  \tgroup by hea.entity_oid \n)\nSELECT\n    lnc.name as \"VM Name\",\n    -- lnc.tags as \"Tags\",\n    replace(translate(lnc.tags, '{}[]\"', ''), ',', ' --- ') as \"Tags\",\n    coalesce(a.num_cpus, lnc.num_cpus) as \"Average number of vCPU(s)\",\n    ${VCPU_COST} as \"vCPU Price ($)\"\nfrom last_num_cpus lnc\nleft join avg_num_cpu a on a.oid = lnc.oid\norder by lnc.name ASC",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "savings"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "completed_action",
          "timeColumn": "recommendation_time",
          "timeColumnType": "timestamp",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        },
        {
          "datasource": {
            "type": "postgres",
            "uid": "${DS_TURBO_TIMESCALE}"
          },
          "format": "table",
          "group": [],
          "hide": false,
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "with memory as (\n  SELECT\n    e.name,\n  --m.type,\n    avg(m.capacity)/(1024*1024) as \"value\",\n    ${VMEM_COST} as \"price\"\n  FROM metric m\n  JOIN entity e ON e.oid = m.entity_oid\n  WHERE\n    $__timeFilter(time)\n    AND entity_oid IN (${VM_OID})\n    AND m.type = 'VMEM'\n  GROUP BY e.name, m.type\n  ORDER BY 1\n)\nselect\n  distinct e.name as \"VM Name\",\n  coalesce(m.value,0) as \"vMem Average (GB)\",\n  m.price as \"GB vMem Price ($)\"\nfrom entity e\nleft outer join memory m on m.name = e.name \nwhere e.oid IN (${VM_OID})\n\n",
          "refId": "B",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        },
        {
          "datasource": {
            "type": "postgres",
            "uid": "${DS_TURBO_TIMESCALE}"
          },
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "with storage as (\n  SELECT\n    e.name,\n--  m.type,\n    avg(m.consumed)/(1024) as \"value\",\n    ${STORAGE_COST} as \"price\"\n    FROM metric m\n    JOIN entity e ON e.oid = m.entity_oid\n    WHERE\n    $__timeFilter(time)\n    AND entity_oid IN (${VM_OID})\n    AND m.type = 'STORAGE_PROVISIONED'\n    GROUP BY e.name, m.type\n    ORDER BY 1\n)\nselect\n  distinct e.name as \"VM Name\",\n  coalesce(s.value,0) as \"Storage Average (GB)\",\n  s.price as \"GB Storage Price ($)\"\nfrom entity e\nleft outer join storage s on s.name = e.name \nwhere e.oid IN (${VM_OID})",
          "refId": "C",
          "select": [
            [
              {
                "params": [
                  "value"
                ],
                "type": "column"
              }
            ]
          ],
          "timeColumn": "time",
          "where": [
            {
              "name": "$__timeFilter",
              "params": [],
              "type": "macro"
            }
          ]
        }
